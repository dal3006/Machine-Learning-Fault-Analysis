    "version": "0.2.0",
    "configurations": [
        {
            "name": "data",
            "type": "python",
            "request": "launch",
            "program": "data.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--batch_size",
                "25",
                "--source",
                "CAL2-NI",
                "--target",
                "CWRUA2-NI",
            ]
        },
        {
            "name": "trainer_main",
            "type": "python",
            "request": "launch",
            "program": "trainer_main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--alpha",
                "0",
                "--beta",
                "0",
                "--source",
                "CWRUA2-NI",
                "--target",
                "CAL2-NI",
                "--num_classes",
                "2",
                "--save_embeddings",
                "false",
                "--experiment_name",
                "CWRUA2-NI_to_CAL2-NI",
                "--learning_rate",
                "1e-3",
                "--accelerator",
                "gpu",
                // "--max_epochs",
                // "1"
            ]
        },
        {
            "name": "grid_search",
            "type": "python",
            "request": "launch",
            "program": "trainer_main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--autorestore",
                "false",
                "--grid_search",
                "true",
                "--save_embeddings",
                "false",
                "--experiment_name",
                "grid_search",
                "--learning_rate",
                "1e-3",
                "--accelerator",
                "gpu",
                "--max_epochs",
                "1"
            ]
